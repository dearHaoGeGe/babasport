<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.my.core.dao.TestTbDao">
	<insert id="insterTestTb" parameterType="TestTb">
		INSERT INTO test_tb (
			test_tb.`name`,
			test_tb.birthday
		)
		VALUES
		(
			#{name},
			#{birthday}
		);
	</insert>
</mapper>